noremap <space> i<space><esc><left>r
noremap ; l
noremap l k
noremap k j
noremap j h
noremap gk gj
noremap gl gk
noremap h ;
noremap c "_c
noremap x "_x
nnoremap <C-w> :tabclose<CR>
nnoremap <C-n> :new<CR>
nnoremap <C-t> :ReopenLastFile<CR>
vnoremap < <gv
vnoremap > >gv

set ignorecase
set smartcase
set winaltkeys=yes
