    "actions": 
    [
        {
            "command": "closePane",
            "keys": "ctrl+w"
        },
        {
            "command": "unbound",
            "keys": "ctrl+shift+w"
        },
        {
            "command": 
            {
                "action": "moveTab",
                "direction": "forward"
            },
            "keys": "alt+shift+period"
        },
        {
            "command": 
            {
                "action": "nextTab"
            },
            "keys": "alt+period"
        },
        {
            "command": 
            {
                "action": "scrollUp"
            },
            "keys": "shift+up"
        },
        {
            "command": 
            {
                "action": "scrollDown"
            },
            "keys": "shift+down"
        },
        {
            "command": 
            {
                "action": "moveTab",
                "direction": "backward"
            },
            "keys": "alt+shift+comma"
        },
        {
            "command": 
            {
                "action": "prevTab"
            },
            "keys": "alt+comma"
        }
    ],
